{
  "name": "api-gym-pass",
  "version": "1.0.0",
  "description": "API de uma aplicação no estilo GymPass desenvolvida em Node.js com TypeScript, Fastify e Prisma, seguindo os princípios SOLID para um código limpo, escalável e de fácil manutenção.",
  "main": "index.js",
  "devDependencies": {
    "@types/node": "24.3.0",
    "@vitest/coverage-v8": "3.2.4",
    "prisma": "6.15.0",
    "tsup": "8.5.0",
    "tsx": "4.20.4",
    "typescript": "5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "start:dev": "tsx watch src/server.ts",
    "start": "node build/server.js",
    "build": "tsup src --out-dir build",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@prisma/client": "4.10.1",
    "@vitest/ui": "^3.2.4",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.1",
    "fastify": "5.5.0",
    "zod": "^4.0.17"
  }
}
